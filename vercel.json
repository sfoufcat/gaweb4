{
  "crons": [
    {
      "path": "/api/notifications/cron/morning",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/notifications/cron/evening",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/notifications/cron/weekly",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/notifications/cron/abandoned",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/notifications/cron/squad-call-reminder",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/notifications/cron/squad-call-jobs",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/notifications/cron/event-recurrence",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/notifications/cron/program-lifecycle",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/stories/cleanup",
      "schedule": "0 * * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "growthaddicts.app" }],
      "destination": "https://growthaddicts.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.growthaddicts.app" }],
      "destination": "https://growthaddicts.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "app.growthaddicts.app" }],
      "destination": "https://app.growthaddicts.com/:path*",
      "permanent": true
    }
  ]
}
